<?xml version="1.0"?>
<DataForm xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xmlns:xsd="http://www.w3.org/2001/XMLSchema"
          xmlns="http://www.enova.pl/schema/form.xsd"
          RightName="Page:ConfigPage"
          Priority="100001"
          DataType="Config">
  <Page Name="ConfigPage" CaptionHtml="Praca Dyplomowa" DataContext="{new Config}">
    <Group CaptionHtml="Import zamówień">
      <Field CaptionHtml="Cecha z numerem paczkomatu" LabelWidth="50" Width="50" EditValue="{TargetPointFeature}"/>
      <Field CaptionHtml="Cecha określająca czy płatność za pobraniem" LabelWidth="50" Width="50" EditValue="{CodFeature}"/>
      <Field CaptionHtml="Domyślna usługa dostawy" LabelWidth="50" Width="50" EditValue="{DefaultDeliveryService}"/>
    </Group>
    <Group CaptionHtml="ShipX">
      <Field CaptionHtml="Numer klienta ShipX" LabelWidth="50" Width="50" EditValue="{ApiUsername}"/>
      <Field CaptionHtml="Token Shipx" LabelWidth="50" Width="50" EditValue="{ApiToken}" Class="PasswordEdit"/>
      <Field CaptionHtml="Cecha z gabarytem paczki" LabelWidth="50" Width="50" EditValue="{ParcelTemplateTypeFeature}"/>
      <Field CaptionHtml="Cecha z numerem śledzenia przesyłki" LabelWidth="50" Width="50" EditValue="{ParcelTrackingNumberFeature}"/>
      <Field CaptionHtml="Cecha z id przesyłki" LabelWidth="50" Width="50" EditValue="{ParcelIdFeature}"/>
      <Field CaptionHtml="Cecha ze statusem przesyłki" LabelWidth="50" Width="50" EditValue="{ParcelStatusFeature}"/>
    </Group>
  </Page>
</DataForm>